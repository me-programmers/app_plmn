<div id="role-options-<%= result %>" class="modal" style="display: none;">
  <%= simple_form_for(@hasil2) do |f| %>
    <div class="modal-header">
      <a class="close" data-dismiss="modal">&#215;</a>
      <h3>Ubah Hasil</h3>
    </div>
    <div class="modal-body">
      <%= f.input :hsl_result %>
    </div>
    <div class="modal-footer">
      <%= f.submit "Ubah", :class => "btn" %>
      <a class="btn" data-dismiss="modal" href="#">Close</a>
    </div>
  <% end %>
</div>